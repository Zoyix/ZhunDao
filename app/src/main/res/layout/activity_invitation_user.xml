<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl_invitation_user"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.zhaohe.zhundao.ui.home.action.signlist.InvitationUserActivity">


    <TextView
        android:clickable="true"

        android:id="@+id/tv_invitation_photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_invitation_photo"
        android:text="@string/app_name" />

    <ImageView
        android:id="@+id/iv_invitation_photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="29dp"
        android:clickable="true"
        android:src="@mipmap/ic_launcher" />
</AbsoluteLayout>
